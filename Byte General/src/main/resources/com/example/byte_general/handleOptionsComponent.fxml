<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" prefWidth="280.0" stylesheets="@style.css">
         <children>
            <TextField prefHeight="34.0" prefWidth="259.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
            <FontIcon iconLiteral="fa-minus" iconSize="20" onMouseClicked="#removeHandle">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></FontIcon>
         </children>
         <VBox.margin>
            <Insets left="35.0" right="50.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
